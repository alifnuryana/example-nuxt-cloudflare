{
  "name": "example-nuxt-cloudflare",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "drizzle-kit generate --dialect=sqlite --out=./server/database/migrations --schema=./server/database/schema.ts",
    "db:migrate": "wrangler d1 migrations apply example-nuxt-cloudflare --local",
    "prepare": "husky"
  },
  "dependencies": {
    "nuxt": "^4.1.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "drizzle-orm": "^0.44.6"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.9.0",
    "eslint": "^9.37.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "wrangler": "^4.42.1",
    "drizzle-kit": "^0.31.5"
  }
}
